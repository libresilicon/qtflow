# profile for non-mevis users to link to PythonQt

include ( python.prf )  

INCLUDEPATH += $$PWD/../src

# check if debug or release
CONFIG(debug, debug|release) {
  DEBUG_EXT = _d 
} else {
  DEBUG_EXT = 
}

win32-msvc* {
  LIBS += $$PWD/$$DESTDIR/PythonQt$${DEBUG_EXT}.lib
} else {
  LIBS += -L$$OUT_PWD/$$DESTDIR
}
